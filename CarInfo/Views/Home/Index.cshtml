@model HomeViewModel
@Html.Partial("Carousel_Card")
<div>
    @{
        foreach (CarBrand brand in Model.PopularBrand)
        {
            @Html.Partial("AllBrands", brand)
        }

      }

</div>
<div class="album py-5 ">
    <div class="container mt-5">
        <div class="row">
            @{
                foreach (Car car in Model.favCars)
                {

                    @Html.Partial("AllCars", car)

                }
            }
        </div>
    </div>
</div>

